{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund --progress=false",
  "framework": "vite",
  "build": {
    "env": {
      "NPM_CONFIG_FUND": "false",
      "NPM_CONFIG_AUDIT": "false",
      "NODE_ENV": "production"
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/iteration1/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/iteration1/(.*)",
      "destination": "/iteration1/index.html"
    },
    {
      "source": "/iteration1",
      "destination": "/iteration1/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), midi=(), accelerometer=(), gyroscope=(), xr-spatial-tracking=(), cross-origin-isolated=()"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/Assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}